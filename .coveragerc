[run]
source = autom8
omit =
    */tests/*
    */venv/*
    */__pycache__/*
branch = True

[report]
precision = 2
show_missing = True
skip_covered = False
exclude_lines =
    pragma: no cover
    def __repr__
    def __str__
    raise AssertionError
    raise NotImplementedError
    if __name__ == "__main__":

[html]
directory = htmlcov

[xml]
output = coverage.xml
